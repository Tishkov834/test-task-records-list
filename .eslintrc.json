{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "@wemake-services/javascript"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "sonarjs/no-small-switch": "off",
        "unicorn/switch-case-braces": "off",
        "no-unused-vars": "off"
    }
}
